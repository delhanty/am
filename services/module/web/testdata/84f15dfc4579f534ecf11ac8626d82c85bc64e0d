if($('.story-wrapper.resource-filter-results').length){
	$(".views-exposed-widget fieldset").hover(
		function() {
			$(this).removeClass('collapsed');
			$(this).find('.fieldset-wrapper').show();
		}, function() {
			$(this).addClass('collapsed');
			$(this).find('.fieldset-wrapper').hide();
		}
	);

	$('.story-wrapper.resource-filter-results').find('input[type="checkbox"]').on('change', function() {
			$('.story-wrapper.resource-filter-results').find('input[type="checkbox"]:not(:checked)').not($(this)).attr('disabled', true);
