$(window).on("load", function(){	/*loadResizeImage();*/	});
function loadResizeImage(){

	$(".story-wrapper__image").each(function( index ) {
	height_img=$(this).height();
	if(height_img > 560){
	if($(this).parent('a').length > 0){

		$(this).closest('div').next('div').css('height',height_img+'px');
		$(this).closest('div').prev('div').css('height',height_img+'px');

		}else{
	$(this).parent().next('div').css('height',height_img+'px');
	$(this).parent().prev('div').css('height',height_img+'px');

	}
	}

	});

}

