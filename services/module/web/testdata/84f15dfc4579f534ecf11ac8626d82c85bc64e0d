$(document).ready(function(){
	if($('.bxslider').length){
		$('.bxslider').bxSlider({
			auto: true,
			autoControls: true,
			pager: false,
			controls: false,
			adaptiveHeight: true
		});
	}
	if($('.bxslider-reason').length){
		$('.bxslider-reason').bxSlider({
			minSlides: 4,
			maxSlides: 5,
			slideWidth: 300,
			slideMargin: 0,
			pager:false,
			onSliderLoad:function(currentIndex){
				setTimeout(function(){
					$(".bx-prev").not('.disabled').wrap('<span class="previous"></span>');
					$(".bx-next").not('.disabled').wrap('<span class="next"></span>');
				}, 2000);
				$('.'+$('.bxslider-reason>li:not(.bx-clone)').eq(0).attr('class').split(' ')[0]).addClass('active');
				var liObj;
				var n;
				var largeImageObj = $('#large-detailed-image img');
				setTimeout(function(){
					$('.cache-large-image').remove();
				}, 5000);
				$('.bxslider-reason li').click(function(){
					liObj = $(this);
					if((largeImageObj.attr('src') != liObj.attr('data-large-image'))){
						largeImageObj.fadeTo( 400, 0.33, function(){
							largeImageObj.clearQueue().stop();
							liObj.clearQueue().stop();
							$('.bxslider-reason li').removeClass('active');
							$('.'+liObj.attr('class').split(' ')[0]).addClass('active');
							$('#large-detailed-image h2').html(liObj.attr('data-heading'));
							$('#large-detailed-image span').html(liObj.attr('data-span'));
							largeImageObj.attr('src', liObj.attr('data-large-image')).fadeTo(400,1);
						});
					}
				});
			}
		});
		/*setSlideHeight();
		$(window).resize(function(){setSlideHeight();});*/
	}


		
/*$(window).on( 'resize', function () {

